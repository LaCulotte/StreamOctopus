  "dependencies": {
    "@types/express": "^4.17.16",
    "@types/node": "^18.0.0",
    "@types/uuid": "^8.3.4",
    "@types/ws": "^8.5.3",
    "@types/yargs": "^17.0.10",
    "dgram": "^1.0.1",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "octopus-app": "^1.0.0",
    "run-script-os": "^1.1.6",
    "ts-node": "^10.8.1",
    "typescript": "^4.9.4",
    "uuid": "^8.3.2",
    "ws": "^8.8.0",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "octopus-app": "file:../octopus-app"
